    "def process_repo_file(file_path, start, end) :\n",
    "    matrix = []\n",
    "    with open(file_path) as file :\n",
    "        for line in file :        \n",
    "            try :             \n",
    "                path = line.strip()\n",
    "                repo = g.get_repo(path)\n",
    "                desc = get_repo_description(repo)\n",
    "                commits = commits_processing(repo, start, end)\n",
    "                row = desc + commits\n",
    "                matrix.append(row)\n",
    "            except:\n",
    "                pass\n",
    "    return matrix"
   ]
